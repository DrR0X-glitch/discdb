{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run SvelteKit Dev Server",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/vite/bin/vite.js",
        "args": ["dev"],
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  